<%- include('/header.ejs', { title: 'Gren Packages', refresh: false, sideButtonType: 'IMPORT' }) -%>
    <form class="inline_form" action="/package/search" method="get">
        <input id="query" name="query" placeholder="Search for package" type="search" autofocus />
        <button type="submit">Search</button>
    </form>

    <h3>Core packages</h3>
    <%- include('/package_list.ejs', { packages: corePackages }) -%>

    <h3>Recently updated</h3>
    <%- include('/package_list.ejs', { packages: recentlyUpdated }) -%>
<%- include('/footer.ejs') -%>
