<%- include('/header.ejs', { title: 'Import Progress', description: null, sideButtonType: 'NONE' }) -%>
    <h1>Package import jobs</h1>

    <table
        id="job_table"
        hx-trigger="every 2s"
        hx-get=""
        hx-select="#job_table"
        hx-target="this"
        hx-swap="outerHTML"
    >
        <tr>
            <th>Name</th>
            <th>Version</th>
            <th>Step</th>
            <th>Retried</th>
            <th>Message</th>
        </tr>
        <%_ jobs.forEach(function(job) { -%>
        <tr>
            <td><%= job.name %></td>
            <td><%= job.version %></td>
            <td><%= job.step %></td>
            <td><%= job.retry %></td>
            <td><%= job.message %></td>
        </tr>
        <%_ }); -%>
    </table>
<%- include('/footer.ejs') -%>
