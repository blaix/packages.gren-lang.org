{
  "name": "package.gren-lang.org",
  "type": "module",
  "version": "1.0.0",
  "description": "The server powering package.gren-lang.org",
  "main": "index.js",
  "imports": {
    "#src/*": "./src/*.js",
    "#db/*": "./src/db/*.js",
    "#routes/*": "./src/routes/*.js"
  },
  "scripts": {
    "start": "node build/index.cjs",
    "build": "esbuild index.js --platform=node --bundle --external:./node_modules/* --outfile=build/index.cjs",
    "dev": "nodemon -e ejs,js,css ./src/app.js",
    "prettier": "prettier -w ."
  },
  "author": "Robin Heggelund Hansen",
  "license": "BSD-3-Clause",
  "engines": {
    "node": "^16.16.0"
  },
  "dependencies": {
    "@koa/router": "^12.0.0",
    "ejs": "^3.1.8",
    "gren-compiler-library": "^0.1.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-compress": "^5.1.0",
    "koa-static": "^5.0.0",
    "markdown-it": "13.0.1",
    "newrelic": "^9.0.0",
    "pino": "^8.4.1",
    "semver": "^7.3.7",
    "sqlite3": "^5.0.8",
    "xdg-basedir": "^5.1.0",
    "zulip-js": "^2.0.9"
  },
  "devDependencies": {
    "esbuild": "^0.15.5",
    "prettier": "2.7.1"
  }
}
